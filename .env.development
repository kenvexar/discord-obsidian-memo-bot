# Development/Mock Environment Configuration
# This file allows testing without real API keys

# Environment Mode
ENVIRONMENT=development
ENABLE_MOCK_MODE=true

# Mock Discord Configuration (development values)
DISCORD_BOT_TOKEN=mock_discord_token_for_development
DISCORD_GUILD_ID=123456789

# Mock API Keys
GEMINI_API_KEY=mock_gemini_api_key_for_development

# Local Obsidian Vault (will be created automatically)
OBSIDIAN_VAULT_PATH=./test_vault

# Mock Channel IDs (development values)
CHANNEL_INBOX=123456789
CHANNEL_VOICE=123456790
CHANNEL_FILES=123456791
CHANNEL_MONEY=123456792
CHANNEL_FINANCE_REPORTS=123456793
CHANNEL_TASKS=123456794
CHANNEL_PRODUCTIVITY_REVIEWS=123456795
CHANNEL_NOTIFICATIONS=123456796
CHANNEL_COMMANDS=123456797
CHANNEL_ACTIVITY_LOG=123456798
CHANNEL_DAILY_TASKS=123456799

# Mock Mode Specific Settings
MOCK_DISCORD_ENABLED=true
MOCK_GEMINI_ENABLED=true
MOCK_GARMIN_ENABLED=true
MOCK_SPEECH_ENABLED=true

# Optional Mock Garmin (not required in mock mode)
GARMIN_EMAIL=mock@example.com
GARMIN_PASSWORD=mock_password

# API Limits (reduced for development)
GEMINI_API_DAILY_LIMIT=100
GEMINI_API_MINUTE_LIMIT=5
SPEECH_API_MONTHLY_LIMIT_MINUTES=10

# Logging
LOG_LEVEL=DEBUG
LOG_FORMAT=console
